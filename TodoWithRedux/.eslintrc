{
  "extends": "airbnb",
  "globals": {
    "document": true,
    "window": true
  },
  "settings": {
    "import/resolver": {
      "node": {
        "paths": ["src"]
      }
    }
  },
  "rules": {
    "react/jsx-indent": ["error", 4],
    "comma-dangle": ["error", "only-multiline"],
    "indent": ["error", 4]
  }
}
